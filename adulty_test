import datetime
from datetime import timedelta

now=datetime.datetime.now()
adult_year=datetime.datetime(now.year-18,now.month,now.day)

birth_date=input("type your birth date (YYYY-MM-DD)")

birth_year,birth_month,birth_day=birth_date.split("-")
birth_year=int(birth_year)
birth_month=int(birth_month)
birth_day=int(birth_day)

if adult_year>=datetime.datetime(birth_year,birth_month,birth_day):
    print("You are adult")
else:
    print("You are not adult")